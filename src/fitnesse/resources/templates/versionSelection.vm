<h2>Select a version</h2>
<table>
 <thead>
  <tr>
   <th>Name</th>
   <th>Author</th>
   <th>Age</th>
   <th>&nbsp;</th>
  </tr>
 </thead>
 <tbody>
  #foreach( $version in $versions )
  <tr>
   <td>$version.name</td>
   <td>$version.author</td>
   <td>$version.age</td>
   <td><a href="?responder=viewVersion&amp;version=$version.name">view</a></td>
  </tr>
  #end
 </tbody>
</table>
